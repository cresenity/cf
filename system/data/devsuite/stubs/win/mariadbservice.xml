<service>
  <id>devsuite_mariadb</id>
  <name>devsuite_mariadb</name>
  <description>DevSuite MariaDB by CF</description>
  <executable>MARIADB_PATH\bin\mysqld.exe</executable>
  <arguments>--defaults-extra-file="MARIADB_INI_FILE" --console</arguments>
  <logpath>DEVSUITE_HOME_PATH\Log\</logpath>
  <logmode>reset</logmode>
  <onfailure action="restart"/>
</service>
